#!/bin/bash

language=$1

case "$language" in
<% for (const language of languages) { -%>
<% if (language.format) { -%>
  "<%- language.name %>")
    cat - > <%- language.entrypoint %>
    <%- language.format.command %>
    ;;

<% } -%>
<% } -%>
  *)
    echo "Unknown language: $language"
    exit 1
    ;;
esac
