#!/bin/bash

language=$1

case "$language" in
<% for (const executor of executors) { -%>
  "<%- executor.name %>")
    cat - > <%- executor.entrypoint %>
<% if (executor.compile) { -%>
    <%- executor.compile.join(' ') %>
<% } -%>
    <%- executor.run.join(' ') %>
    ;;
<% } -%>
  *)
    echo "Unknown language: $language"
    exit 1
    ;;
esac
